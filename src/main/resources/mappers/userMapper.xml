<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace="com.leestagram.user.dao.UserDAO">
    <select id="existLoginId" parameterType="String" resultType="boolean">
        SELECT
            count(1)
        FROM
            `user`
        WHERE
        	`loginId` = #{loginId}
    </select>
    <insert id="insertUser" parameterType="com.leestagram.user.model.User">
    	INSERT INTO `user`
    	(
    		`loginId`
    		,`password`
    		,`name`
    		,`profileUrl`
    		,`createdAt`
    		,`updatedAt`
    	)
    	VALUES
    	(
    		#{loginId}
    		, #{password}
    		, #{name}
    		, #{profileUrl}
    		, NOW()
    		, NOW()
    	) 
    </insert>
 </mapper>